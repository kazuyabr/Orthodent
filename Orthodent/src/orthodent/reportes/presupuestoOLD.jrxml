FROM
     `tratamiento_piezapresupuesto` tratamiento_piezapresupuesto
WHERE
     tratamiento_piezapresupuesto.`id_presupuesto` = $P{ID_PRESUPUESTO}]]>
		</queryString>
		<field name="tratamiento_piezapresupuesto_pieza" class="java.lang.Integer"/>
